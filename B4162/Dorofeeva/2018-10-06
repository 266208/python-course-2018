class Music :
 classattribute = "classatribute"
 def method(self) :
     print("This is main class")
class Rock(Music) :
    r_attr = "Rock"
    def method(self) :
        print("This is music genre")
class Gothic(Music) :
    g_attr = "Gothic"
    def method(self) :
        print("This is music genre")
class Metal(Rock) :
    m_attr = "Metal"
    def method(self) :
        print("Metal is a kind of rock")
class Hard(Rock) :
    h_attr = "Hard"
    def method(self) :
        print("Hard is a kind of rock")
class Alternative(Metal) :
    a_attr = "Alternative"
    def method(self) :
        print("Alternative is a kind of metal")
class AlternativeMetal(Alternative, Metal) :
    am_attr = "AlternativeMetal"
    def method(self) :
        print(self.a_attr, self.m_attr)
c1 = AlternativeMetal()
c1.method()
class GothicMetal(Gothic, Metal) :
    gm_attr = "GothicMetal"
    def method(self) :
        print(self.g_attr, self.m_attr)
c2 = GothicMetal()
c2.method()
class HardRock(Hard, Rock) :
    hr_attr = "HardRock"
    def method(self) :
        print(self.h_attr, self.r_attr)
c3 = HardRock()
c3.method()
class Rammstein(AlternativeMetal, GothicMetal, HardRock) :
    def method(self) :
        print(self.am_attr, self.gm_attr, self.hr_attr)
        print("Rammstein is alternative metal, gothic metal and hard rock music band")
c4 = Rammstein()
c4.method()

